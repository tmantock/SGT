{
  "name": "NodeReactStarter",
  "version": "1.0.0",
  "description": "A simple starter for using Node to serve and compile react using webpack as middleware",
  "main": "server.js",
  "scripts": {
    "start": "webpack && node server.js",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "webpack -w",
    "watch:server": "nodemon --ext js,jsx --ignore public/ server.js"
  },
  "author": "Tevin Mantock",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.26.1",
    "elemental": "^0.6.1",
    "express": "^4.13.4",
    "firebase": "^2.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^4.1.1",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "reactfire": "^1.0.0",
    "redux": "^3.6.0",
    "redux-form": "^5.0.1",
    "redux-thunk": "^2.1.0",
    "sass": "^0.5.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
